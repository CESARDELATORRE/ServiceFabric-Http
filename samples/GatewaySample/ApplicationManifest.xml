<?xml version="1.0" encoding="utf-8"?>
<ApplicationManifest xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" ApplicationTypeName="GatewaySampleType" ApplicationTypeVersion="1.0.0" xmlns="http://schemas.microsoft.com/2011/01/fabric">
   <Parameters>
      <Parameter Name="HttpServiceService_InstanceCount" DefaultValue="2" />
      <Parameter Name="HttpGatewayService_InstanceCount" DefaultValue="1" />
   </Parameters>
   <ServiceManifestImport>
      <ServiceManifestRef ServiceManifestName="HttpService" ServiceManifestVersion="1.0.0" />
   </ServiceManifestImport>
   <ServiceManifestImport>
      <ServiceManifestRef ServiceManifestName="HttpGateway" ServiceManifestVersion="1.0.0" />
   </ServiceManifestImport>
   <DefaultServices>
      <Service Name="HttpServiceService">
         <StatelessService ServiceTypeName="HttpServiceType" InstanceCount="[HttpServiceService_InstanceCount]">
            <SingletonPartition />
         </StatelessService>
      </Service>
      <Service Name="HttpGatewayService">
         <StatelessService ServiceTypeName="HttpGatewayType" InstanceCount="[HttpGatewayService_InstanceCount]">
            <SingletonPartition />
         </StatelessService>
      </Service>
   </DefaultServices>
</ApplicationManifest>